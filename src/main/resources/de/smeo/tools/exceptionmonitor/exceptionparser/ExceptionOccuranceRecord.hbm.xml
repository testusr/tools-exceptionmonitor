<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.smeo.tools.exceptionmonitor.exceptionparser">

	<class name="ExceptionOccuranceRecord" table="EMON_EXPOCCURANCE">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		
        <property name="filename"/>
        <property name="filePosition"/>
        <property name="time"/>
	</class>

</hibernate-mapping>